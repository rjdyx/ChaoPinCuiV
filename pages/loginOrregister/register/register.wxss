.register{
  width: 100%;
  height: 100vh;
  background-color: #F2f2f2;
  padding: 0;
  margin: 0;
  display: flex;
  align-items: center;
  flex-direction: column;
}
.inputWrap{
  width: 100%; 
  height: 328rpx;
  /* background-color: #fff; */
  margin: 0 auto;
  display: flex;
  align-items: center;
  flex-direction: column;
  margin-top: 50rpx;
  margin-bottom: 300rpx;
}
.inputWrap input{
  width: 92%;
  height: 105rpx;
  padding: 30rpx;
  display: flex;
  margin-bottom: 3rpx;
  background-color: #fff;
}
.inputWrap input:first-child{
  border-radius: 25rpx 25rpx 0 0;
}
.inputWrap input:last-child{
  border-radius: 0 0 25rpx 25rpx;
}
.check{
  width: 100%;
  display: flex;
  align-items: center;
  flex-direction: row;
  margin: 0 auto;
  height: 118rpx;
  background-color: #fff;
  border-radius: 14rpx;
  margin-bottom: 50rpx;
}
.check input{
  width: 57%;
  height: 100%;
  padding-left: 30rpx;
  background: #fff;
  border-radius: 25rpx 0 0 25rpx;
}
.nosend{
  background:  #ffd102;
  border:none; 
  height: 100%;
  width: 39%;
  border-radius: 0 14rpx 14rpx 0;
  line-height: 111rpx;
  text-align: center;
}
.hassend{
  background:  #ddd;
  border:none; 
  height: 100%;
  width: 39%;
  border-radius: 0 14rpx 14rpx 0;
  line-height: 111rpx;
  text-align: center;
}
.registerBtn{
   width: 100%;
  height: 118rpx;
  line-height: 118rpx;
  margin: 0 auto;
}
.normal{
  background-color: #ddd;
}
.is_checked{
  background-color: #ffd102;
}
.items{
  width: 100%;
  margin: 0 auto;
  margin-top: 20rpx;
  font-size: 32rpx;
  color: #919191;
}
